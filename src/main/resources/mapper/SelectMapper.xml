<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bigbrother.game.domain.mapper.TestMapper">
    <select id="queryAgencyList" parameterType="com.bigbrother.game.domain.pojo.Agency"
            resultMap="com.bigbrother.game.domain.pojo.Agency">
        SELECT * FROM ad.t_agency
        <where>
            <trim prefixOverrides="and">
                <if test="city_id != null">
                    city_id = #{city_id};
                </if>
                <if test="agency_name != null">
                    and agency_name = #{agency_name};
                </if>
                <if test="agency_address != null">
                    and agency_address = #{agency_address};
                </if>
                <if test="connect_phone != null">
                    and connect_phone = #{connect_phone};
                </if>
                <if test="create_time != null">
                    and create_time = #{create_time};
                </if>
            </trim>
        </where>
    </select>

    <select id="queryUserList" parameterType="com.bigbrother.game.domain.pojo.User"
            resultMap="com.bigbrother.game.domain.pojo.User">
        SELECT * FROM ad.t_users
        <where>
            <trim prefixOverrides="and">
                <if test="user_name != null">
                    user_name = #{user_name};
                </if>
                <if test="phone != null">
                    and phone = #{phone};
                </if>
                <if test="sex != null">
                    and sex = #{sex};
                </if>
                <if test="soft_del != null">
                    and soft_del = #{soft_del};
                </if>
            </trim>
        </where>
    </select>

    <select id="queryOrderList" parameterType="com.bigbrother.game.domain.pojo.Order"
            resultMap="com.bigbrother.game.domain.pojo.Order">
        SELECT * FROM ad.order_detail
        <where>
            <trim prefixOverrides="and">
                <if test="user_name != null">
                    user_name = #{user_name};
                </if>
                <if test="user_phone != null">
                    and user_phone = #{user_phone};
                </if>
                <if test="user_sex != null">
                    and user_sex = #{user_sex};
                </if>
                <if test="agency_id != null">
                    and agency_id = #{agency_id};
                </if>
                <if test="city_id != null">
                    and city_id = #{city_id};
                </if>
                <if test="province_id != null">
                    and province_id = #{province_id};
                </if>
                <if test="car_type_id != null">
                    and car_type_id = #{car_type_id};
                </if>
                <if test="channel_id != null">
                    and channel_id = #{channel_id};
                </if>
                <if test="create_time != null">
                    and create_time = #{create_time};
                </if>
                <if test="update_time != null">
                    and update_time = #{update_time};
                </if>
                <if test="status != null">
                    and status = #{status};
                </if>
            </trim>
        </where>
    </select>
</mapper>